{
  "security.allowedUNCHosts": [
    "wsl.localhost"
  ],
  "remote.autoForwardPortsSource": "hybrid",
  "background.windowBackgrounds": [
    "FILEPATHHERE"
  ],
  "material-icon-theme.folders.associations": {},
  "materialTheme.accent": "Pink",
  "workbench.startupEditor": "none",
  "workbench.colorTheme": "Material Theme Darker",
  "workbench.productIconTheme": "material-product-icons",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorCustomizations": {
    "sideBar.foreground": "#ffffff85",
    "tab.inactiveForeground": "#ffffff85",
    "editorError.foreground": "#ff0f0fb9",
    // "editorWarning.foreground": "#fffc37c5"
    "editor.selectionBackground": "#a200ff91",
    "editor.selectionHighlightBackground": "#4300695d",
    "editor.findMatchBackground": "#ffe60070", //Current SEARCH MATCH
    "editor.findMatchHighlightBackground": "#ffe60023" //Other SEARCH MATCHES
  },
  // "terminal.integrated.automationProfile.linux": {},
  "terminal.integrated.defaultProfile.linux": "debian",
  "terminal.integrated.fontFamily": "JetBrains Mono",
  "terminal.integrated.fontWeight": "bold",
  "terminal.integrated.profiles.linux": {
    "debian": {
      "path": "wsl.exe",
      "args": [
        "-d",
        "Debian",
        "--exec",
        "zsh",
        "-l"
      ],
      "icon": "terminal-debian",
      "useWslProfile": true,
      "overrideName": true,
      "name": "Debian",
    },
  },
  "github.copilot.editor.enableAutoCompletions": true,
  "git.openRepositoryInParentFolders": "never",
  "background.autoInstall": true,
  "debug.inlineValues": "off",
  "diffEditor.ignoreTrimWhitespace": false,
  "explorer.confirmDragAndDrop": false,
  "editor.wordWrap": "on",
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontWeight": "1000",
  "editor.fontLigatures": true,
  "editor.fontVariations": false,
  "editor.stickyScroll.enabled": false,
  "editor.renderWhitespace": "none",
  "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/intellij-java-google-style.xml",
  "java.format.settings.profile": "GoogleStyle",
  "java.format.enabled": true,
  "java.configuration.updateBuildConfiguration": "automatic",
  "[java]": {
    "editor.formatOnSave": false,
    // "editor.formatOnSaveMode": "modifications",
    "editor.formatOnPaste": false,
    // "editor.defaultFormatter": "josevseb.google-java-format-for-vs-code",
    "editor.tabSize": 2,
    "editor.inlayHints.enabled": "off",
    // "editor.autoIndent": "advanced",
    // "editor.detectIndentation": true
  },
  "[csharp]": {
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "modifications",
    "editor.formatOnPaste": true,
    "editor.tabSize": 2,
    "editor.inlayHints.enabled": "off",
    "editor.autoIndent": "advanced",
    "editor.detectIndentation": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
  },
  "[ruby]": {
    // "editor.defaultFormatter": "",
    "editor.formatOnType": false,
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "modifications",
    "editor.formatOnPaste": true,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": false,
    "editor.rulers": [
      120
    ],
  },
  // The ruby LSP extension is not working as of 12/11/2024
  // "rubyLsp.rubyExecutablePath": "/home/jlaws44/run_ruby_lsp.sh",
  // "rubyLsp.formatter": "rubocop",
